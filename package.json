{
  "name": "flatsheet-notes",
  "description": "a microblogging client for flatsheet",
  "version": "0.1.0",
  "repository": {
    "url": "git://github.com/flatsheet/flatsheet-notes.git"
  },
  "main": "index.js",
  "scripts": {
    "test": "",
    "watch": "watchify index.js -o bundle.js -t brfs",
